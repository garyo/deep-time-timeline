{
  "name": "log-timeline",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test-timeline": "node --loader tsx log-timeline.ts",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx,js,jsx,json,md,astro}'",
    "format:check": "prettier --check '{src,test}/**/*.{ts,tsx,js,jsx,json,md,astro}'"
  },
  "dependencies": {
    "@astrojs/solid-js": "5.1.0",
    "@tailwindcss/vite": "^4.1.7",
    "astro": "5.8.0",
    "d3": "^7.9.0",
    "solid-js": "^1.9.7",
    "tailwindcss": "^4.1.7",
    "temporal-polyfill": "^0.3.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@vitest/ui": "^3.1.4",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "^4.16.5",
    "vitest": "^3.1.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none",
    "plugins": [
      "prettier-plugin-astro"
    ]
  }
}
